{
  "name": "@circlesland/capacitor-host",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:ios": "npm run build && cap sync && cap run ios",
    "dev:ios": "concurrently \"vite --host\" \"npm run capacitor:dev:ios\"",
    "capacitor:start:ios": "npm run capacitor:sync:ios && npm run capacitor:run:ios",
    "capacitor:dev:ios": "cross-env IS_DEV=true npm run capacitor:sync:ios && npm run capacitor:run:ios",
    "capacitor:sync:ios": "cap sync ios",
    "capacitor:run:ios": "cap open ios",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@capacitor/app": "^1.1.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "3.0.4"
  },
  "dependencies": {
    "@capacitor/cli": "^3.6.0",
    "@capacitor/core": "^3.6.0",
    "@capacitor/ios": "^3.6.0",
    "@circlesland/frame-app": "^0.0.8"
  }
}
